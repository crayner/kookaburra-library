{
    "name": "kookaburra/library",
    "type": "symfony-bundle",
    "description": "Kookaburra Library Bundle",
    "keywords": [
        "doctrine"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Rayner",
            "email": "craig@craigrayner.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Kookaburra\\Library\\": "/src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('vendor/kookaburra/src/Resources/config/dist/library.yaml', 'config/packages/kookaburra_library.yaml');\""
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kookaburra\\Library\\Tests\\": "tests/"
        }
    }
}
