{
    "name": "kookaburra/library",
    "type": "symfony-bundle",
    "description": "Kookaburra Library Bundle",
    "keywords": [
        "doctrine"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Rayner",
            "email": "craig@craigrayner.com"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "install-module": {
            "module:install 'Library'": "symfony-cmd"
        },
        "post-install-cmd": [
            "@install-module"
        ],
        "post-update-cmd": [
            "@install-module"
        ]
    },
    "autoload": {
        "psr-4": {
            "Kookaburra\\Library\\": "/src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kookaburra\\Library\\Tests\\": "tests/"
        }
    }
}
